{
  "name": "dhcp-mon",
  "version": "1.0.4",
  "description": "NodeJS implementation of DHCP socket connection",
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.2",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rollup": "^1.21.4",
    "rollup-plugin-dts": "^0.15.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "test": "mocha",
    "build": "rollup -c",
    "prepatch": "npm run build",
    "patch": "npm version patch",
    "pub": "npm run patch && npm publish && git push",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrustedPlus/dhcp-mon.git"
  },
  "keywords": [
    "dhcp",
    "net",
    "mac",
    "server",
    "client",
    "socket"
  ],
  "author": "",
  "contributors": [
    "Miroshin Stepan<microshine@mail.ru>",
    "Protasova Elena<lendjeina@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrustedPlus/dhcp-mon/issues"
  },
  "homepage": "https://github.com/TrustedPlus/dhcp-mon#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
